{
  "name": "ol-plus",
  "version": "1.1.10",
  "description": "Interactive plugin to Openlayers",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "doc": "jsdoc -c jsdoc.json",
    "build-package": "npm run transpile && node tasks/prepare-package && npm run copyCSS && npm run copyFonts",
    "build-index": "npm run build-package && node tasks/generate-index",
    "build-legacy": "shx rm -rf build && npm run build-index && webpack --config config/webpack-config-legacy-build.js",
    "transpile": "shx rm -rf build/ol && shx mkdir -p build/ol && shx cp -rf src/ol build/ol/src && tsc --project config/tsconfig-build.json",
    "dev": "webpack serve --config example/webpack.dev.config.js --hot",
    "copyCSS": "find ./src/ol -name '*.css' -exec bash -c 'dir=\"$2$(sed \"s,^$3,,;s,/[^/]\\+$,,\" <<< \"$1\")\"; mkdir -p \"$dir\"; cp -v \"$1\" \"$dir\"' _ {} ./build/ol ./src/ol \\;",
    "copyFonts": "cp -rf ./src/ol/fonts ./build/ol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suman111sanjel/ol-plus.git"
  },
  "keywords": [
    "Openlayers",
    "mapping",
    "ol"
  ],
  "author": "suman sanjel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/suman111sanjel/ol-plus/issues"
  },
  "homepage": "https://github.com/suman111sanjel/ol-plus#readme",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.13.15",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "buble": "^0.20.0",
    "buble-loader": "^0.5.1",
    "clean-webpack-plugin": "*",
    "core-js": "^2.6.12",
    "css-loader": "^5.2.5",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.3.1",
    "jsdoc": "^3.6.6",
    "jsdoc-plugin-typescript": "^2.0.6",
    "ol": "^6.5.0",
    "ol-plus": "^1.0.0",
    "shx": "^0.3.3",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.4",
    "walk": "^2.3.14",
    "webpack": "^5.33.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "bootstrap-slider": "^11.0.2",
    "jquery": "^3.6.0",
    "ol-ext": "^3.2.3",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0"
  }
}
